{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": [
      "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": 2016,
      "sourceType": "module"
  },
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],

  "rules": {
    "prettier/prettier": "warn",
    "no-console": "warn",
    "space-before-function-paren": "off",
    "no-useless-constructor": "off",
    "camelcase":"off",
		"indent": "off",
		"brace-style": "off"
  }

}
